<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <com.harshil258.adplacer.adViews.NativeBigView
        android:id="@+id/myAdViewBig"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:shouldLoadDirectBig="false" />

    <com.harshil258.adplacer.adViews.NativeSmallView
        android:layout_marginVertical="@dimen/_5sdp"
        android:id="@+id/myAdViewSmall"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:shouldLoadDirectSmall="false" />

</RelativeLayout>